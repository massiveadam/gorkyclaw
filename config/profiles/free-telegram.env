# Free + Telegram default profile
# Copy these into your local .env and fill in required values.

TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN_HERE
TELEGRAM_ADMIN_CHAT_ID=YOUR_TELEGRAM_CHAT_ID_HERE

ANTHROPIC_BASE_URL=http://127.0.0.1:3000
ANTHROPIC_API_KEY=YOUR_OPENROUTER_KEY_HERE
OPENROUTER_API_KEY=YOUR_OPENROUTER_KEY_HERE

REQUIRE_FREE_MODELS=true
REASONING_MODEL=google/gemma-3-27b-it:free
COMPLETION_MODEL=google/gemma-3-27b-it:free
FALLBACK_MODELS=meta-llama/llama-3.3-70b-instruct:free,mistralai/mistral-small-3.1-24b-instruct:free

ENABLE_APPROVED_EXECUTION=true
APPROVED_ACTION_WEBHOOK_URL=http://127.0.0.1:8080/dispatch
APPROVED_ACTION_WEBHOOK_SECRET=CHANGE_ME_WEBHOOK_SECRET
APPROVED_ACTION_WEBHOOK_TIMEOUT_MS=10000
ENABLE_LOCAL_APPROVED_EXECUTION=false

OPS_RUNNER_SHARED_SECRET=CHANGE_ME_RUNNER_SECRET
WEB_RESEARCH_ENRICHMENT=true
